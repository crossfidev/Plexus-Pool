(library
 (name mineplex_baking_alpha)
 (public_name mineplex-baking-alpha)
 (libraries mineplex-base
            mineplex-version
            mineplex-protocol-alpha
            mineplex-protocol-environment
            mineplex-shell-context
            mineplex-shell-services
            mineplex-client-base
            mineplex-client-alpha
            mineplex-client-commands
            mineplex-stdlib-unix
            mineplex-storage
            mineplex-rpc-http
            mineplex-rpc
            lwt-canceler)
 (library_flags (:standard -linkall))
 (modules (:standard \
                     delegate_commands
                     delegate_commands_registration))
 (flags (:standard -open mineplex_base__TzPervasives
                   -open mineplex_protocol_alpha
                   -open mineplex_shell_services
                   -open mineplex_client_base
                   -open mineplex_client_alpha
                   -open mineplex_client_commands
                   -open mineplex_stdlib_unix
                   -open mineplex_shell_context
                   -open mineplex_storage
                   -open mineplex_rpc
                   -open mineplex_rpc_http)))

